local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("JxstHub | Total Roblox Drama", "Synapse")

local Tab1 = Window:NewTab("Main")
local Section1 = Tab1:NewSection("Section Name")

local Tab2 = Window:NewTab("Player")
local Section2 = Tab2:NewSection("Speed")
Section2:NewTextBox("Character speed", "Type in a number of speed you want to have", function(txt)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = txt
end)
Section3:NewKeybind("Toggle UI", "makes the gui invisible, press again to make it visible", Enum.KeyCode.RightAlt, function()
	Library:ToggleUI()
end)
local Section3 = Tab2:NewSection("Others")
Section3:NewButton("Infinite Yield", "Just infinte yield", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

local Tab3 = Window:NewTab("Teleport")
local Section4 = Tab3:NewSection("Win")
Section4:NewButton("Finish", "Teleport you to finish on click", function()
	local LocalPlayer = game.Players.LocalPlayer.Character.HumanoidRootPart
	local PartName = "Finish"
	local Part = game:GetService("Workspace"):FindFirstChild(PartName, true)
	if Part then
		local TweenService = game:GetService("TweenService")
		local tweenInfo = TweenInfo.new(3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
		local tween = TweenService:Create(LocalPlayer, tweenInfo, { CFrame = Part.CFrame })
		tween:Play()
	end	
end)
local Section5 = Tab3:NewSection("Statue")
Section5:NewButton("Bag", "Teleport you to bag on click", function()
	local LocalPlayer = game.Players.LocalPlayer.Character.HumanoidRootPart
	local ModelName = "Bag"
	local Model = game:GetService("Workspace"):FindFirstChild(ModelName, true)
	if Model and Model:IsA("Model") then
		local RootPart = Model:FindFirstChildWhichIsA("BasePart")
		if RootPart then
			local TweenService = game:GetService("TweenService")
			local tweenInfo = TweenInfo.new(3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tween = TweenService:Create(LocalPlayer, tweenInfo, { CFrame = RootPart.CFrame })
			tween:Play()
		end
	end	
end)
Section5:NewButton("Safety Statue", "Teleport you to safety statue on click", function()
	local LocalPlayer = game.Players.LocalPlayer.Character.HumanoidRootPart
	local ModelName = "SafetyStatue"
	local Model = game:GetService("Workspace"):FindFirstChild(ModelName, true)
	if Model and Model:IsA("Model") then
		local RootPart = Model:FindFirstChildWhichIsA("BasePart")
		if RootPart then
			local TweenService = game:GetService("TweenService")
			local tweenInfo = TweenInfo.new(3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tween = TweenService:Create(LocalPlayer, tweenInfo, { CFrame = RootPart.CFrame })
			tween:Play()
		end
	end	
end)
